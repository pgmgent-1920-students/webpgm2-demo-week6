{
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 2019,
        "sourceType": "module"
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "rules":{
        "linebreak-style": 0,
        "no-plusplus":"off",
        "no-console":"off",
        "class-methods-use-this":"off"
    }
}